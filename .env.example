# JWT secret for signing tokens (generate a strong random string, min 32 chars)
JWT_SECRET=your-jwt-secret-change-me-in-production

# Neon Postgres (direct connection for serverless queries)
DATABASE_URL=postgresql://user:password@ep-your-endpoint.us-east-2.aws.neon.tech/dbname?sslmode=require

# Site URL (for auth redirects)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

